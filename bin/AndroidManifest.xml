<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hairetc.hairetcapp1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.hairetc.hairetcapp1.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.SPLASH" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hairetc.hairetcapp1.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.hairetc.hairetcapp1.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hairetc.hairetcapp1.Form"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.hairetc.hairetcapp1.FORM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hairetc.hairetcapp1.ClientSignIn"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.hairetc.hairetcapp1.SIGNIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hairetc.hairetcapp1.SecondActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>